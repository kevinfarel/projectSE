<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#42f489"
    tools:context="com.example.kevinfarel.myapplication.MainMenuPrinter">
<TextView
    android:id="@+id/statview"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="15dp"
    android:layout_gravity="right"
    android:layout_marginRight="150dp"
    />
<Switch
    android:id="@+id/statswitch"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:layout_gravity="right"
    android:layout_marginRight="20dp"
    />
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="Setup"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="60dp"
        android:onClick="PindahSetupPrinter"/>
    <Button
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:text="Order\nList"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="200dp"
        android:onClick="PindahOrderList"/>
</FrameLayout>
